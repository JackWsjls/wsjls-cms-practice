{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true,
    "jasmine": true,
    "mocha": true
  },
  "root": true,
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx":false
  },
  // "extends": "eslint-config-airbnb",
  "globals": {
    "DEBUG": false,
    "$utils": false,
    "$application": false,
    "$commonConfigs": false,
    "$environment": false,
    "$userInfo": false,
    "$init": false,
    "getJsBridge": false,
    "deepCopy": false,
    "ajax": false,
    "getYYYYMMDD": false,
    "getMMDD": false,
    "getHHMM": false,
    "isIdentityCard": false,
    "wx": false,
    "getParameterByName": false,
    "ActiveXObject": false
  },
  "rules": {
    "comma-spacing": 4,
    "camelcase": 1,
    "curly": [2, "all"],
    "dot-notation": 2,
    "indent": [2, 4],
    "max-len": [0, 80],
    "new-cap": 2,
    "no-loop-func": 0,
    "no-underscore-dangle": 2,
    "no-unused-expressions": [2, { "allowShortCircuit": true, "allowTernary": true }],
    "no-unused-vars": [2, { "vars": "all", "args": "none" }],
    "no-var": 1,
    "no-undef": 2,
    "no-caller": 1,
    "quotes": [2, "single", "avoid-escape"],
    "space-before-blocks": 1,
    "brace-style": 2,
    "keyword-spacing": [2, { "before": true, "after": true }],
    "comma-dangle": [2, "never"],
    "newline-after-var": [0, "always"],
    "one-var": [2, "never"],
    "no-eval": 0,
    "no-console": 1,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-floating-decimal": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-redeclare": 2,
    "default-case": 2,
    "semi": [2, "always"],
    "semi-spacing": [2, {"before": false, "after": true}],
    "no-extra-semi": 0,
    "eol-last": 2,
    "eqeqeq": 0,
    "func-names": 2,
    "guard-for-in": 0,
    "vars-on-top": 0,
    "radix": 0,
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "no-await-in-loop": 2,
    "no-compare-neg-zero": 2,
    "no-control-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-extra-parens": 0,
    "linebreak-style": 0,
    "padded-blocks": 0,
    "template-curly-spacing": 0,
    "quote-props": 0,
    "no-magic-numbers": 0,
    "no-ternary": 0,
    "object-curly-spacing": 0,
    "arrow-parens": 0,
    "no-plusplus": 0,
    "space-unary-ops": 2,
    "id-length": 0,
    "lines-around-comment": 0,
    "max-statements": 0,
    "multiline-ternary": 0,
    "sort-keys": 0,
    "object-curly-newline": 0,
    "global-require": 0,
    "no-implicit-coercion": 0,
    "valid-jsdoc": 0,
    "callback-return": 0,
    "newline-before-return": 0,
    "no-trailing-spaces": 2,
    "no-irregular-whitespace": 2,
    "prefer-template": 0,
    "no-unmodified-loop-condition": 0,
    "spaced-comment": 2,
    "no-continue": 0,
    "prefer-const": 2,
    "key-spacing": 2,
    "init-declarations": 2,
    "no-empty-function": 0,
    "newline-per-chained-call": 0,
    "no-multi-spaces": 2,
    "consistent-return": 0,
    "no-invalid-this": 2,
    "complexity": 0,
    "prefer-destructuring": 0,
    "no-template-curly-in-string": 0,
    "capitalized-comments": 0,
    "no-global-assign": 0,
    "space-infix-ops": 2,
    "space-before-function-paren": 2,
    "require-jsdoc": 0,
    "max-params": 0,
    "no-useless-escape": 2,
    "no-inline-comments": 0,
    "line-comment-position": 0,
    "wrap-regex": 0,
    "no-else-return": 0,
    "no-confusing-arrow": 0,
    "no-eq-null": 0,
    "yoda": 0,
    "no-negated-condition": 0,
    "no-use-before-define": 2,
    "object-property-newline": 0,
    "no-alert": 0,
    "no-mixed-operators": 0,
    "space-in-parens": 2,
    "no-shadow": 0,
    "no-warning-comments": 0,
    "max-depth": 0,
    "no-unreachable": 0,
    "max-lines": 0,
    "require-await": 0,
    "for-direction": 0,
    "no-new": 0,
    "no-extra-boolean-cast": 0,
    "no-ex-assign": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-obj-calls": 2,
    "no-prototype-builtins": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "accessor-pairs": 2,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "class-methods-use-this": 2,
    "dot-location": 2,
    "no-case-declarations": 2,
    "no-div-regex": 2,
    "no-empty-pattern": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-fallthrough": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-multi-str": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-restricted-properties": 2,
    "no-return-assign": 2,
    "no-return-await": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-labels": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-return": 2,
    "no-void": 2,
    "no-with": 2,
    "prefer-promise-reject-errors": 2,
    "wrap-iife": 2,
    "strict": 2,
    "no-catch-shadow": 0,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-restricted-globals": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "handle-callback-err": 2,
    "no-mixed-requires": 2,
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-process-env": 0,
    "no-process-exit": 2,
    "no-restricted-modules": 2,
    "no-sync": 0,
    "array-bracket-spacing": 2,
    "block-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "consistent-this": 2,
    "func-call-spacing": 2,
    "func-name-matching": 2,
    "id-blacklist": 2,
    "id-match": 2,
    "jsx-quotes": 2,
    "lines-around-directive": 2,
    "max-nested-callbacks": 2,
    "max-statements-per-line": 2,
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-assign": 2,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-restricted-syntax": 2,
    "no-tabs": 2,
    "no-unneeded-ternary": 2,
    "no-whitespace-before-property": 2,
    "nonblock-statement-body-position": 2,
    "one-var-declaration-per-line": 2,
    "operator-assignment": 2,
    "operator-linebreak": 2,
    "sort-vars": 2,
    "template-tag-spacing": 2,
    "unicode-bom": 2,
    "arrow-body-style": 2,
    "arrow-spacing": 2,
    "constructor-super": 2,
    "generator-star-spacing": 2,
    "no-class-assign": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-new-symbol": 2,
    "no-restricted-imports": 2,
    "no-this-before-super": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "prefer-arrow-callback": 2,
    "prefer-numeric-literals": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "require-yield": 2,
    "rest-spread-spacing": 2,
    "sort-imports": 2,
    "symbol-description": 2,
    "yield-star-spacing": 2,
    "no-empty-character-class": 2,
    "no-labels": 2,
    "func-style": [2, "expression", { "allowArrowFunctions": true }]
  }
}
